<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable
        name="AnimateTextView"
        >
        <attr name="start"
            format="integer"/>
        <attr name="end"
            format="integer"/>
        <attr name="textSize"
            format="integer"
            />
        <attr name="textColor"
            format="reference"/>
        <attr name="duration"
            format="integer"/>
        <attr name="maxwidthmode">
            <enum name="end" value="1"/>
            <enum name="wrap" value="2"/>
        </attr>
        <attr name="mode"
            >
            <enum name="cashout" value="2"/>
            <enum name="animate" value="3"/>
            <enum name="downcount" value="1"/>
        </attr>
        <attr name="textonleft" format="string"/>
        <attr name="textonright" format="string"/>
    </declare-styleable>
</resources>